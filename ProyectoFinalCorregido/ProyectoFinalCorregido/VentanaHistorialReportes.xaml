<Window x:Class="ProyectoFinalCorregido.VentanaHistorialReportes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProyectoFinalCorregido"
        mc:Ignorable="d"
        Title="Histoial de reportes" Height="450" Width="500">
    <Grid>
        <ListView HorizontalAlignment="Left" Height="280" Margin="25,65,0,0" VerticalAlignment="Top" Width="450" FontSize="14">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Label Content="Lista de reportes emitidos" HorizontalAlignment="Left" Margin="25,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="14"/>
        <Button x:Name="btn_regresar" Content="Regresar" HorizontalAlignment="Left" Margin="400,10,0,0" VerticalAlignment="Top" Width="75" FontSize="14" Click="btn_regresar_Click"/>
    </Grid>
</Window>
